<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="95.0" prefWidth="202.0" stylesheets="@/css/style.css,@/css/icons.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.components.BoardCardPreviewCtrl">
    <Pane onMouseClicked="#openBoard" prefHeight="105.0" prefWidth="202.0" styleClass="boardCard">
      <children>
         <Label fx:id="boardTitle" layoutX="12.0" layoutY="11.0" prefHeight="33.0" prefWidth="73.0" styleClass="boardCardLabel" text="Label" />
         <Label fx:id="boardDescription" layoutX="9.0" layoutY="40.0" prefHeight="66.0" prefWidth="146.0" styleClass="boardCardLabelDescription" text="Description" />
          <Pane onMouseClicked="#editBoard" layoutX="130.0" layoutY="15.0" prefHeight="22.0" prefWidth="26.0" styleClass="edit"/>
         <Pane onMouseClicked="#leaveOrDeleteBoard" layoutX="162.0" layoutY="17.0" prefHeight="22.0" prefWidth="26.0" styleClass="trash">
         </Pane>
      </children>
    </Pane>
</AnchorPane>
